{
  "name": "movies-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^5.12.2",
    "date-fns": "^2.30.0",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-detect-offline": "^2.4.5",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "lint:fix": "eslint --fix",
